{
  "extends": [
    "config:base",
    "schedule:monthly"
  ],
  "timezone": "America/Detroit",
  "vulnerabilityAlerts": {
    "labels": ["security"],
    "schedule": "at any time"
  },
  "assignees": ["@emmercm"],
  "npmrc": "@fortawesome:registry=https://npm.fontawesome.com/\n//npm.fontawesome.com/:_authToken=${NPM_TOKEN}",
  "encrypted": {
    "npmToken": ""
  },
  "rangeStrategy": "bump",
  "groupName": "Back-End",
  "packageRules": [
    {
      "packageNames": [
        "@fortawesome/fontawesome-pro",
        "bootstrap",
        "highlight.js",
        "jquery",
        "lozad",
        "mobile-detect"
      ],
      "groupName": "Front-End"
  ]
    },
    { "packageNames": ["metalsmith-renamer"], "allowedVersions": "!/^0.5.0$/" },
    { "packageNames": ["metalsmith-twitter-card"], "allowedVersions": "!/^1.0.6$/"}
}
